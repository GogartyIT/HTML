<div id="box">
	<div id="box_text">
      <div>
        <p>
          <span id="text"></span><span class="cursor"></span>
        </p>
      </div>
      
      <script>
        const text = "I am interested in all things IT, Crypto and Technology\n\nNeed Help with IT on a Charity or Volunteer project,\nI would be glad to hear from you and have a Coffee";
        const speed = 75; // in milliseconds
      
        let i = 0;
        let terminal = document.querySelector("#text").parentNode;
        let textElement = document.querySelector("#text");
        let cursorElement = document.querySelector(".cursor");
      
        function typeText() {
  if (i < text.length) {
    let char = text.charAt(i);
    if (char === "\n") {
      textElement.innerHTML += "<br>";
    } else {
      // Create a temporary span element to calculate the width of the character
      let charElement = document.createElement("span");
      charElement.textContent = char;
      charElement.style.visibility = "hidden";
      document.body.appendChild(charElement);
      let charWidth = charElement.offsetWidth;
      document.body.removeChild(charElement);
      
      textElement.innerHTML += "<span style='color: green;'>" + char + "</span>";
      cursorElement.style.marginLeft = (textElement.offsetWidth + charWidth + 4) + "px";
    }
    i++;
    setTimeout(typeText, speed);
  } else {
    cursorElement.style.display = "none";
  }

  terminal.scrollTop = terminal.scrollHeight; // auto-scroll to bottom
}

      
        setTimeout(typeText, speed);
      </script>
        
      
  </div>
